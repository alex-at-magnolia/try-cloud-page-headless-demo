{
  "name": "vue-minimal",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy:mgnl": "npm run clean && npm run build && copyfiles -u 1 \"dist/**/*\" ../../light-modules/vue-minimal-lm/webresources/dist",
    "clean": "rimraf dist && rimraf ../../light-modules/vue-minimal-lm/webresources/dist"
  },
  "dependencies": {
    "@magnolia/vue-editor": "^1.0.0",
    "core-js": "^3.6.5",
    "vue": "^3.0.0",
    "vue-router": "^4.0.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "babel-eslint": "^10.1.0",
    "copyfiles": "^2.4.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "rimraf": "^2.7.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
